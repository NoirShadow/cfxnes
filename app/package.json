{
  "name": "cfxnes-app",
  "version": "0.5.0",
  "description": "cfxnes emulator",
  "license": "MIT",
  "private": true,
  "keywords": [
    "emulator",
    "nes"
  ],
  "author": "Jan Pikl <pikl.jan@gmail.com>",
  "homepage": "https://github.com/jpikl/cfxnes",
  "bugs": {
    "url": "https://github.com/jpikl/cfxnes/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jpikl/cfxnes"
  },
  "scripts": {
    "start": "node dist",
    "build": "parallelshell 'npm run build:server' 'npm run build:client'",
    "dev": "parallelshell 'npm run dev:server' 'npm run dev:client'",
    "build:server": "NODE_ENV=production babel -d dist src/server",
    "build:client": "NODE_ENV=production webpack -p --progress",
    "dev:server": "NODE_ENV=development nodemon -w 'src/server' -x 'babel-node src/server'",
    "dev:client": "NODE_ENV=development webpack-dev-server -d --progress --inline --hot --open",
    "lint": "eslint --ext=.js,.jsx src || true",
    "clean": "rm -rf dist",
    "prelint": "node tools/check-lib --include-debug",
    "prebuild": "node tools/check-lib",
    "prebuild:client": "node tools/check-lib",
    "predev": "node tools/check-lib --include-debug",
    "predev:client": "node tools/check-lib --include-debug"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "eslint": "^3.13.0",
    "eslint-import-resolver-webpack": "^0.8.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.7.0",
    "morgan": "^1.7.0",
    "nodemon": "^1.11.0",
    "parallelshell": "^2.0.0",
    "postcss": "^5.2.9",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.2.1",
    "postcss-url": "^5.1.2",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.0-rc.3",
    "webpack-dev-server": "^2.2.0-rc.0",
    "whatwg-fetch": "^2.0.1"
  },
  "dependencies": {
    "express": "^4.14.0"
  }
}
