{
  "name": "cfxnes-app",
  "version": "0.6.0",
  "description": "NES emulator",
  "license": "MIT",
  "private": true,
  "keywords": [
    "emulator",
    "nes"
  ],
  "author": "Jan Pikl <pikl.jan@gmail.com>",
  "homepage": "https://github.com/jpikl/cfxnes",
  "bugs": {
    "url": "https://github.com/jpikl/cfxnes/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jpikl/cfxnes"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "start": "node dist",
    "build": "parallelshell 'npm run build:server' 'npm run build:client'",
    "dev": "parallelshell 'npm run dev:server' 'npm run dev:client'",
    "build:server": "NODE_ENV=production babel -d dist src/server",
    "build:client": "NODE_ENV=production webpack -p --progress",
    "dev:server": "NODE_ENV=development nodemon -w 'src/server' -x 'babel-node src/server'",
    "dev:client": "NODE_ENV=development webpack-dev-server -d --devtool=eval-source-map --progress --inline --hot --open",
    "lint": "parallelshell 'npm run lint:js' 'npm run lint:css'",
    "lint:js": "NODE_ENV=development eslint --ext=.js,.jsx src",
    "lint:css": "stylelint 'src/client/**/*.css'",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.7.0",
    "lodash-es": "^4.17.4",
    "morgan": "^1.7.0",
    "nodemon": "^1.11.0",
    "parallelshell": "^2.0.0",
    "postcss": "^5.2.11",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.2.2",
    "postcss-url": "^5.1.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "source-map-loader": "^0.1.6",
    "style-loader": "^0.13.1",
    "stylelint": "^7.7.1",
    "stylelint-config-standard": "^15.0.1",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0",
    "whatwg-fetch": "^2.0.2"
  },
  "dependencies": {
    "express": "^4.14.0"
  }
}
